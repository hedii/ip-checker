{
    "name": "hedii/ip-checker",
    "description": "Check whether an ip address can be used in a Laravel application",
    "homepage": "https://github.com/hedii/ip-checker",
    "license": "MIT",
    "authors": [
        {
            "name": "hedii",
            "email": "contact@hedichaibi.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/console": "5.6.*",
        "illuminate/contracts": "5.6.*",
        "illuminate/notifications": "5.6.*",
        "illuminate/support": "5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Hedii\\IpChecker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hedii\\IpChecker\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hedii\\IpChecker\\IpCheckerServiceProvider"
            ]
        }
    }
}
